@model ToDuo.Models.ViewModels.MyProfileVM
@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <link rel="stylesheet" type="text/css" href="~/Content/Custom/Shared/SharedClasses.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Custom/Index/Dashboard.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Custom/MyProfile/MyProfile.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
}
}

<div id="BodyContent" class="sectionContainer">


    <div id="LeftNavBar" class="container">
        <ul>
            <a href="/Home/MyProfile">
            <li class="LeftIconOptionContainer"><i class="fa fa-group LeftSideOptionIcon"></i>My Friends</li></a>
            <li class="LeftIconOptionContainer " id="LikedSwipeLeftMenutChoice"><i class="fa fa-exchange LeftSideOptionIcon"></i>My Liked Swipes</li>
            <li class="LeftIconOptionContainer" id="MatchSwipeLeftMenuChoice"><i class="fa fa-fire LeftSideOptionIcon"></i>ToDuo Matches</li>
            <li class="LeftIconOptionContainer "><i class="fa fa-trello LeftSideOptionIcon"></i>My Boards</li>
            <li class="LeftIconOptionContainer "><i class="fa fa-product-hunt LeftSideOptionIcon"></i>My profile</li>

        </ul>
    </div>

    <div id="RightSection">
        <div id="RightSideContentContainer" class="col-md-10">
            <div id="RightSideContent" class="row">

                <div class="col-md-12" id="MiddleSectionContent">
                    <div class="col-md-12"><h4 id="MiddleLabel">Friends</h4></div>
                    <!--FriendCards-->
                    @Html.Partial("~/Views/Home/MyProfile/Middle/MiddleSection.cshtml", Model)


                </div>
            </div>

            <div id="RightSideContentSearchContainer" class="col-md-2">
                <div id="RightSideSearchContent">
                    <div id="SearchBarHolder" class="col-md-12">
                        <input type="text" id="RightSearchBar" class="form-control" placeholder="Search..." />
                        <i class="fa fa-search RightSideSearchIcon"></i>
                    </div>

                    <div id="" class="col-md-12">
                        <h4 class="bold rightSideHeading">Friends</h4>
                        <ul>
                            <li class="RightIconOptionContainer activeLeftIcon"><i class="fa fa-group LeftSideOptionIcon"></i>My Friends</li>
                            <li class="RightIconOptionContainer"><i class="fa fa-exchange LeftSideOptionIcon"></i>My Liked Swipes</li>
                            <li class="RightIconOptionContainer "><i class="fa fa-trello LeftSideOptionIcon"></i>My Boards</li>
                            <li class="RightIconOptionContainer "><i class="fa fa-product-hunt LeftSideOptionIcon"></i>My profile</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>


    </div>
    @*    @Html.Partial("~/Views/Home/Dashboard/Modals/AddAdventure.cshtml")
        @Html.Partial("~/Views/Home/Dashboard/Modals/_DetailsAdventure.cshtml")
        @Html.Partial("~/Views/Home/Dashboard/Modals/_MoreInfoAdventureModal.cshtml")*@

    @Html.Partial("~/Views/Home/MyProfile/Modals/SearchFriendModal.cshtml", Model)
    @Html.Partial("~/Views/Home/Dashboard/Modals/_DetailsAdventure.cshtml")
    @Html.Partial("~/Views/Home/Dashboard/Modals/_MoreInfoAdventureModal.cshtml")

    <script type="module" src="~/Scripts/Custom/MyProfile/View/MyFriends.js"></script>
    <script type="module" src="~/Scripts/Custom/MyProfile/View/MyLikedSwipes.js"></script>
    <script type="module" src="~/Scripts/Custom/MyProfile/View/MyMatches.js"></script>

</div>

